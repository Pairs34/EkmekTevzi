  object qSumBagis: TUniQuery
    Connection = dbHelper
    SQL.Strings = (
      
        'select SUM((select Adet from users where users.KartId = hareket.' +
        'KartId)) as '#39'Adet'#39' from hareket'
      
        'where strftime('#39'%Y'#39',IslemZamani) = :Yil and strftime('#39'%m'#39',IslemZ' +
        'amani) = :Ay')
